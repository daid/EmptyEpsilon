#: scripts/scenario_30_brokenglass.lua:1519
msgctxt "adminStnEndTips-comms"
msgid "Ban wormhole research"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1520
msgctxt "adminStnEndTips-comms"
msgid "I just can't stand wormholes, or their researchers!"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1523
msgctxt "adminStnEndTips-comms"
msgid "Heros for the ages"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1524
msgctxt "adminStnEndTips-comms"
msgid "Their reputation is unmatched"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1527
msgctxt "adminStnEndTips-comms"
msgid "Grand Slam"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1528
msgctxt "adminStnEndTips-comms"
msgid "You can do it, but can you do it ALL?"
msgstr ""

#: scripts/scenario_30_brokenglass.lua:1532
msgctxt "adminStnEnd-comms"
