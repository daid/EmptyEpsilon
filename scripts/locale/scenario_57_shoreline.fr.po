#: scripts/scenario_57_shoreline.lua:1066
#: scripts/scenario_57_shoreline.lua:4622
msgctxt "scienceDescription-station"
msgid "Mining and trade"
msgstr "Exploitation minière et commerce"

#: scripts/scenario_57_shoreline.lua:1104
#: scripts/scenario_57_shoreline.lua:4659
msgctxt "scienceDescription-station"
msgid "Mining and gambling"
msgstr "Exploitation minière et jeux de hasard"

#: scripts/scenario_57_shoreline.lua:1143
#: scripts/scenario_57_shoreline.lua:1698
#: scripts/scenario_57_shoreline.lua:1817
