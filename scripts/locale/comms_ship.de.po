msgctxt "shipAssist-comms"
msgid "Hull: %d%%\n"
msgstr "Hülle: %d%%\n"

#: scripts/comms_ship.lua:190
#, lua-format
msgctxt "shipAssist-comms"
msgid "Shield: %d%%\n"
msgstr "Schilde: %d%%\n"

#: scripts/comms_ship.lua:192
#, lua-format
