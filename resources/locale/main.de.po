msgctxt "ship"
msgid "Ktlitan Feeder"
msgstr ""

#: scripts/shipTemplates_OLD.lua:523
msgctxt "ship"
msgid "Ktlitan Scout"
msgstr ""

#: scripts/shipTemplates_OLD.lua:529
msgctxt "ship"
msgid "Ktlitan Destroyer"
msgstr ""

#: scripts/shipTemplates_OLD.lua:540
msgctxt "ship"
msgid "Ktlitan Queen"
