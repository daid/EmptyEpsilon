#: scripts/shipTemplates_Corvette.lua:14 scripts/shipTemplates_Corvette.lua:83
#: scripts/shipTemplates_Corvette.lua:101
#: scripts/shipTemplates_Corvette.lua:157
#: scripts/shipTemplates_Corvette.lua:217
